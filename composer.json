{
    "require": {
        "php": ">=7.2",
        "slim/slim": "^3.0",
        "slim/psr7": "^1.0.0",
        "monolog/monolog": "^2.0",
        "illuminate/database": "^5.8",
        "endroid/qr-code": "^4.6",
        "phpmailer/phpmailer": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "YourNamespace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourNamespace\\Tests\\": "tests/"
        }
    }
}

